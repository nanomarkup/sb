use go

define:
	Src github.com/sapplications/sbuilder/src
	LogDefault 0
	LogTrace 1
	LogDebug 2
	LogInfo 3
	LogWarn 4
	LogError 5
	LogOff 6

apps:
	sb {Src}/cmd.SmartBuilder
	sgo {Src}/plugins/sgo.Plugin

github.com/hashicorp/go-plugin.HandshakeConfig:
	ProtocolVersion 1
	MagicCookieKey "SMART_PLUGIN"
	MagicCookieValue "sbuilder"